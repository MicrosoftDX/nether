    <Content Include="wwwroot\Features\identity-ui\css\site.min.css" />
    <Content Include="wwwroot\Features\identity-ui\favicon.ico" />
    <Content Include="wwwroot\Features\identity-ui\icon.jpg" />
    <Content Include="wwwroot\Features\identity-ui\icon.png" />
    <Content Include="wwwroot\Features\identity-ui\js\signout-redirect.js" />
    <Content Include="wwwroot\Features\identity-ui\lib\bootstrap\css\bootstrap.css" />
    <Content Include="wwwroot\Features\identity-ui\lib\bootstrap\css\bootstrap.css.map" />
    <Content Include="wwwroot\Features\identity-ui\lib\bootstrap\css\bootstrap.min.css" />
    <Content Include="wwwroot\Features\identity-ui\lib\bootstrap\fonts\glyphicons-halflings-regular.eot" />
    <Content Include="wwwroot\Features\identity-ui\lib\bootstrap\fonts\glyphicons-halflings-regular.svg" />
    <Content Include="wwwroot\Features\identity-ui\lib\bootstrap\fonts\glyphicons-halflings-regular.ttf" />
    <Content Include="wwwroot\Features\identity-ui\lib\bootstrap\fonts\glyphicons-halflings-regular.woff" />
    <Content Include="wwwroot\Features\identity-ui\lib\bootstrap\fonts\glyphicons-halflings-regular.woff2" />
    <Content Include="wwwroot\Features\identity-ui\lib\bootstrap\js\bootstrap.js" />
    <Content Include="wwwroot\Features\identity-ui\lib\bootstrap\js\bootstrap.min.js" />
    <Content Include="wwwroot\Features\identity-ui\lib\jquery\jquery.js" />
    <Content Include="wwwroot\Features\identity-ui\lib\jquery\jquery.min.js" />
    <Content Include="wwwroot\Features\identity-ui\lib\jquery\jquery.min.map" />
    <Content Include="wwwroot\Features\identity-ui\lib\oidc-client\.bower.json" />
    <Content Include="wwwroot\Features\identity-ui\lib\oidc-client\bower.json" />
    <Content Include="wwwroot\Features\identity-ui\lib\oidc-client\dist\oidc-client.js" />
