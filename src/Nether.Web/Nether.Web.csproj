    <Content Include="wwwroot\Features\identity-ui\lib\oidc-client\jsconfig.json" />
    <Content Include="wwwroot\Features\identity-ui\lib\oidc-client\LICENSE" />
    <Content Include="wwwroot\Features\identity-ui\lib\oidc-client\oidc-client.d.ts" />
    <Content Include="wwwroot\Features\identity-ui\lib\oidc-client\package.json" />
    <Content Include="wwwroot\Features\identity-ui\lib\oidc-client\README.md" />
    <Content Include="wwwroot\Features\identity-ui\lib\oidc-client\webpack.base.js" />
  </ItemGroup>

  <ItemGroup>
    <Content Update="wwwroot\**\*;Views;Areas\**\Views;appsettings.json;web.config;Nether.Web.Xml">
      <CopyToPublishDirectory>PreserveNewest</CopyToPublishDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
